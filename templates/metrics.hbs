# HELP prevent_start DEPRECATED - migrate alarms to do_not_run_generator
# TYPE prevent_start gauge
prevent_start {{#if do_not_run_generator}}1{{else}}0{{/if}}

# HELP generator_do_not_run If the generator override is applied. 1 if override is on, 0 if off.
# TYPE generator_do_not_run gauge
generator_do_not_run {{#if do_not_run_generator}}1{{else}}0{{/if}}

# HELP auto_shore_limit The current shore limit that generator_manager will set when the generator is off.
# TYPE auto_shore_limit gauge
auto_shore_limit {{shore_limit}}

# HELP generator_started_by_generator_manager If generator manager turned on the generator and are responsible for turning it off.
# TYPE generator_started_by_generator_manager gauge
generator_started_by_generator_manager {{#if we_turned_it_on}}1{{else}}0{{/if}}

# HELP generator_on_timer_seconds How much longer is on the generator timer, generally due to low voltage.
# TYPE generator_on_timer_seconds gauge
generator_on_timer_seconds {{generator_on_timer_seconds}}

# HELP generator_wanted If the generator is wanted by the SoC configuration.
# TYPE generator_wanted gauge
generator_wanted {{#if generator_wanted}}1{{else}}0{{/if}}
